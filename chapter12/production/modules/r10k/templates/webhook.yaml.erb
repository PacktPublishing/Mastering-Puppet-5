---
<% @webhook_hash.sort.each do |key,value| -%>
<% if value != :undef and ! value.nil? -%>
<%
  value = case value
  when TrueClass, FalseClass
    value
  when Array
    value.sort.inspect.to_s
  when /^:/
    value.to_sym
  else
    "\"#{value}\""
  end
-%>
<%=key-%>: <%= value %>
<% end -%>
<% end -%>
